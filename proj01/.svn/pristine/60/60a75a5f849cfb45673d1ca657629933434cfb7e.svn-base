public class World {
    
    private Room startRoom;
    private Room endRoom;
    
    public World(Room startRoom, Room endRoom){
        this.startRoom = startRoom;
        this.endRoom   = endRoom;
    }
    
    public Room getStartingRoom(){
        return startRoom;
    }
            
    public Room getEndingRoom(){
        return endRoom;
    }
    
    
    
    
    
    
    
    
    
    
    
    
    
