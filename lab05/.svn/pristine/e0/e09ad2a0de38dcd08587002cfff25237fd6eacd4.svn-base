    private void submitButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_submitButtonActionPerformed
        // TODO add your handling code here:
        String noun1 = noun1TextField.getText();
        String noun2 = noun2TextField.getText();
        String noun3 = noun3TextField.getText();
        String noun4 = noun4TextField.getText();
        String verb = verbTextField.getText();
        String adverb = adverbTextField.getText();
        String preposition = prepositionTextField.getText();

        outputTextArea.setText("One time, I " + verb + " a " + noun1 + " in my kitchen. It was fun at first, but it " + adverb + " caused problems "
                + preposition + " " + noun2 + "." + " My " + noun3 + " didn't like it very much and made me go to " + noun4 + ".");


    }//GEN-LAST:event_submitButtonActionPerformed

